<div id="email-header" class="site-header">

    <div class="email-constrain row">

        <table>
        <tbody>
            <tr>
                <td id="email-header-logo">
                    <a href="{{ site.url }}" rel="home"><img alt="{{ site.name }}" src="{{ site.live_urls.theme }}/assets/images/logos/logo.png" /></a>
                </td>
            </tr>
            {% if not hide_social_header_icons %}
            <tr>
                <td id="email-header-social">
                    {% include 'partials/emails/social-icons.twig' %}
                </td>
            </tr>
            {% endif %}
        </tbody>
        </table>

    </div>

</div>
