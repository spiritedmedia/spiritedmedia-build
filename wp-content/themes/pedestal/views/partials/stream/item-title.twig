{% spaceless %}
{% set title = title|default( item.get_the_title ) %}
{% set url = url|default( item.get_the_permalink ) %}

{% if item.get_type == 'link' %}
{% set link_atts = 'target="_blank"' %}
{% endif %}

<h3 class="c-stream__item__title">
    <a href="{{ url }}" class="c-stream__item__link  js-entity-permalink" {{ link_atts }}
        data-ga-category="{{ gaCategory }}"
        data-ga-label="Title|{{ title }}|Type: {{ item.get_type_name|capitalize }}">
            {{ title }}
    </a>
</h3>

{% endspaceless %}
